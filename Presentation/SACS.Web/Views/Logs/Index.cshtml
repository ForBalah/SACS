@model IEnumerable<string>

@{
    ViewBag.Title = "Server Logs";
}

@section Breadcrumbs {
    <li>@Html.ActionLink("Home", "Index", "Server")</li>
    <li>@Html.ActionLink("Server", "Index", "Server")</li>
    <li class="active"><i class="fa fa-book"></i> Logs</li>
}

<!-- Logs box -->
<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Available Log Files</h3>
        <div class="box-tools pull-right">
            <a href="@Url.Action("Index")" class="btn"><span class="glyphicon glyphicon-refresh"></span> Refresh</a>
        </div>
    </div>
    <div class="box-body">
        @Html.Partial("_LogList", Model)
    </div><!-- /.box-body -->
    <div class="box-footer">
        <small><strong>Note:</strong> Logs can be downloaded from the server.</small>
    </div><!-- /.box-footer-->
</div><!-- /.box -->